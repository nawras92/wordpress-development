{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "lwn-blocks/lwn-dynamic-block",
	"version": "0.1.0",
	"title": "Lwn Dynamic Block",
	"category": "widgets",
	"icon": "dashboard",
	"description": "Simple Block to explain the inner blocks",
	"example": {},
	"supports": {
		"html": false
	},
	"attributes": {
		"content": {
			"type": "string",
			"default": "Text....."
		},
		"tagName": {
			"type": "string",
			"default": "p"
		}
	},
	"textdomain": "lwn-dynamic-block",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js"
}
